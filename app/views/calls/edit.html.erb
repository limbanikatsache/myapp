
<p>Edit a contact</p>



<%= form_for @call do |f| %>

<p><%= f.label :name %></p>	
<p><%= f.collection_select(:number, @contacts, :id, :name, :include_blank => "Please select") %></p>

<p><%= f.label :time %></p>
<p><%= f.text_field :time  %></p>

<p><%= f.submit %></p>

<% end %>



<p><a href="/calls/show">View Contacts</a></p>


